#!/bin/bash

# 获取当前目录
basepath=/Users/larry/Documents/note/wiki/bin
cd $basepath

set -e

echo "preparing..."
python rename.py
python tags.py
python nav.py

echo "mkdocs build --clean"
echo "clean & building .... "
cd ..
mkdocs build --clean -q --site-dir=/Library/WebServer/Documents/
echo "building complete!"
